@{
    Layout = null;
}
    <h2>Conversation</h2>
    <form name="theForm">
        <div class="form-horizontal">
            <ul id="validationSummary"></ul>
            <div class="form-group">
                <label for="TimeCreated" class = "control-label col-md-2"> TimeCreated </label>
                <div class="col-md-10">
                    <input type="text" ng-model="data.TimeCreated" id="TimeCreated"  class="form-control"/>
                    <div id="validation_for_TimeCreated"></div>
                </div>
            </div>
    
            <div class="form-group">
                <label for="Subject" class = "control-label col-md-2"> Subject </label>
                <div class="col-md-10">
                    <input type="text" ng-model="data.Subject" id="Subject"  class="form-control"/>
                    <div id="validation_for_Subject"></div>
                </div>
            </div>
    
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="button" value="Save" class="btn btn-default" ng-click="save()" />
                </div>
            </div>
        </div>
    </form>
    
    <div>
        <a href="#/Conversation">Back to List</a>
    </div>
