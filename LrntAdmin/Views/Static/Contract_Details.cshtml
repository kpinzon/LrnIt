@{
    Layout = null;
}
    <h2>Contract Details Test</h2>
        <div class="form-horizontal">
            <ul id="validationSummary"></ul>
            <div class="form-group">
                <label for="IsAccepted" class = "control-label col-md-2">
                IsAccepted
                </label>
                <div class="col-md-10">
                    {{data.IsAccepted}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Rate" class = "control-label col-md-2">
                Rate
                </label>
                <div class="col-md-10">
                    {{data.Rate}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Description" class = "control-label col-md-2">
                Description
                </label>
                <div class="col-md-10">
                    {{data.Description}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Owner" class = "control-label col-md-2">
                Owner
                </label>
                <div class="col-md-10">
                    {{data.Owner}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Users" class = "control-label col-md-2">
                 Users
                </label>
                <div class="col-md-10">
                    <a ng-href="#User?Contract={{data.Id }}" >
                    {{data.Users_Count}}
                    </a>
                </div>
            </div>
    
           
        </div>
    
    <div>
        <a href="#/Contract">Back to List</a>
    </div>
